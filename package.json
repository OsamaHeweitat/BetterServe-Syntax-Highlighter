{
  "name": "betterserve-syntax-highlighter",
  "displayName": "BetterServe Syntax Highlighter",
  "description": "Provides syntax highlighting for the BetterServe language",
  "repository": {
    "type": "git",
    "url": "}",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.99.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "bs",
        "aliases": [
          "BetterServe",
          "bs"
        ],
        "extensions": [
          ".bs",
          ".cql"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "bs",
        "scopeName": "source.bs",
        "path": "./syntaxes/bs.tmLanguage.json"
      }
    ]
  },
  "dependencies": {
    "betterserve-syntax-highlighter": "file:"
  }
}
